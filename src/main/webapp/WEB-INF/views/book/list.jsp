<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<html>
<head>
    <title>List of books</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

</head>
<body>
<div id="wrapper">

    <div class="container">
        <img class="rounded mx-auto d-block mt-5" src="data:image/png;base64,">
        <div class="btn-group btn-group-toggle mx-auto mt-5" data-toggle="buttons">
            <a class="btn btn-secondary active">Books</a> <a class="btn btn-primary" href="/propositions">Propositions</a> <a class="btn btn-primary" href="/authors">Authors</a> <a class="btn btn-primary" href="/publishers">Publishers</a>
        </div>
        <div class="card card-register mx-auto mt-5">
            <div class="card-header">List of books</div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                        <tr>
                            <th>Title</th>
                            <th>Authors</th>
                            <th>Rating</th>
                            <th>Publisher</th>
                            <th>Description</th>
                            <th>Action</th>
                        </tr>
                        </thead>
                        <tbody>
                            <c:forEach items="${books}" var="book">
                                <c:if test="${book.proposition == false}">
                                <tr>
                                    <td><c:out value="${book.title}" /></td>
                                    <td><c:forEach items="${book.authors}" var="author">
                                        <c:out value="${author.fullName}" /><br>
                                    </c:forEach></td>
                                    <td><c:out value="${book.rating}" /></td>
                                    <td><c:out value="${book.publisher.name}" /></td>
                                    <td><c:out value="${book.description}" /></td>
                                    <td>
                                        <a class="btn btn-primary" href="/books/edit/${book.id}">Edit</a> <a class="btn btn-primary" href="/books/delete/${book.id}">Delete</a></td>
                                </tr>
                                </c:if>
                            </c:forEach>
                        </tbody>
                    </table>
                </div>
                <a class="btn btn-primary" href="/books/add">Add Book</a>
            </div>
        </div>
    </div>
</div>


    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>

</body>
</html>
